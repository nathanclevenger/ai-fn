{
  "name": "ai-functions",
  "type": "module",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "test:e2e": "vitest --config vitest.config.e2e.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test && npm run test:e2e",
    "build": "tsc",
    "format": "prettier ."
  },
  "dependencies": {
    "@ai-sdk/openai": "latest",
    "ai": "latest",
    "dotenv": "^16.3.1",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@vitest/coverage-v8": "latest",
    "@vitest/ui": "latest",
    "prettier": "^3.4.2",
    "typescript": "^5.3.3",
    "vite": "latest",
    "vitest": "latest"
  },
  "prettier": {
    "printWidth": 160,
    "semi": false,
    "singleQuote": true
  }
}